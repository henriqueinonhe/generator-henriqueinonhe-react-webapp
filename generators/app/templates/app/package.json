{
    "jest": {
        "roots": [
            "YOUR_TESTS_LOCATION"
        ],
        "transform": {
            "^.+\\.[jt]sx?$": "ts-jest"
        },
        "collectCoverageFrom": [
            "YOUR_TESTS_LOCATION"
        ],
        "testURL": "http://localhost:3000",
        "preset": "jest-puppeteer"
    },
    "scripts": {
        "dev-start": "webpack serve --env.NODE_ENV=development",
        "prod-start": "webpack serve --env.NODE_ENV=development",
        "unit-test": "jest -t=\\[Unit\\] --collectCoverage",
        "integration-test": "jest -t=\\[Integration\\]",
        "ui-test": "jest -t=\\[UI\\]",
        "test": "jest --collectCoverage",
        "dev-build": "webpack --env.NODE_ENV=development",
        "prod-build": "webpack --env.NODE_ENV=production",
        "docs": "rm docs -rf && typedoc"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/plugin-transform-modules-commonjs": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@types/express": "^4.17.7",
        "@types/jest": "^25.2.3",
        "@types/jest-environment-puppeteer": "^4.3.2",
        "@types/node": "^14.10.1",
        "@types/puppeteer": "^3.0.2",
        "@types/react": "^16.9.48",
        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "eslint": "^7.9.0",
        "eslint-plugin-react": "^7.20.6",
        "expect-puppeteer": "^4.4.0",
        "file-loader": "^6.0.0",
        "jest": "^25.5.4",
        "jest-puppeteer": "^4.4.0",
        "nodemon": "^2.0.4",
        "puppeteer": "^5.3.0",
        "ts-jest": "^25.5.1",
        "ts-loader": "^7.0.5",
        "typedoc": "^0.17.8",
        "typescript": "^3.9.7",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.5.2",
        "webpack-livereload-plugin": "^2.3.0"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "styled-components": "^5.1.1"
    }
}
